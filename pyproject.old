[tool.poetry]
name = "agents"
packages = [
    { include = "accounts" }
    ]
version = "0.1.0"
description = "Experimentations with agents on Langgraph"
authors = ["Boris Tvaroska <boris@tvaroska.sk>"]
readme = "README.md"

[tool.poetry.dependencies]
numpy = "<2.0.0"
grpcio = "1.67.1"
feedparser = "^6.0.11"
tqdm = "^4.67.1"
ipywidgets = "^8.1.5"


[tool.poetry.group.dev.dependencies]
langgraph-cli = "^0.1.65"
jupyterlab = "^4.3.4"
langgraph-sdk = "^0.1.48"

[build-system]
requires = ["poetry-core"]
build-backend = "poetry.core.masonry.api"
